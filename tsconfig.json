{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./build",
      "esModuleInterop": true,
      "strict": true,
      "sourceMap": true,
      "strictPropertyInitialization": false,
      "jsx": "react"
  },
  "include": [
      "src/**/*.ts",
      "src/**/*.jsx",
      "src/**/*.tsx"
  ],
  "exclude": [
      "node_modules",
      ".vscode"
  ]
}